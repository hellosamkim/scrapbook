<section class="contact-create">
  <h2><%= @contact.first_name %> <%= @contact.last_name %></h2>

  <table class="table" style="width:340px">
    <tr>
      <td>Phone Number: </td>
      <td><%= @contact.phone_number %></td>
    </tr>
    <tr>
      <td>E-Mail: </td>
      <td><%= @contact.email %></td>
    </tr>
    <tr>
      <td>Social Media: </td>
      <td><%= @contact.social_media %></td>
    </tr>
  </table>
</section>